<?xml version="1.0" encoding="UTF-8"?>
<service id="remoting-service" class="flex.messaging.services.RemotingService">
	<adapters>
		<adapter-definition id="java-object" class="flex.messaging.services.remoting.adapters.JavaAdapter" default="true"/>
	</adapters>

	<default-channels>
		<channel ref="my-amf"/>
	</default-channels>
	
	<destination id="classifiedManager">
		<properties>
	  		<factory>spring</factory> 
	  		<source>classifiedManager</source> 
	  	</properties>
  	</destination>
  	
  	<destination id="userManager">
		<properties>
	  		<factory>spring</factory> 
	  		<source>userManager</source> 
	  	</properties>
  	</destination>
  	
  	<destination id="mentorManager">
		<properties>
	  		<factory>spring</factory> 
	  		<source>mentorManager</source> 
	  	</properties>
  	</destination>
  	
  	<destination id="menteeManager">
		<properties>
	  		<factory>spring</factory> 
	  		<source>menteeManager</source> 
	  	</properties>
  	</destination>
  	

</service>